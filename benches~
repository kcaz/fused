from pylab import *
from matplotlib import pyplot
a=np.zeros((len(range(-4,5)),len(list(np.linspace(0,1,10)))))

lnlamS=range(-4,5)
pctfused=list(np.linspace(0,1,10))

#for b_err fig:
for i in range(len(range(-4,5))):
    for j in range(10):
        nnzlamS=benchdict[(lnlamS[i],pctfused[j])].values()[0][0]
        zlamS=benchdict2[(0,pctfused[j])].values()[0][0]
        a[(i,j)]=nnzlamS-zlamS
matshow(a)
colorbar()
show()

a=np.zeros((len(range(-4,5)),len(list(np.linspace(0,1,10)))))
lamSlab=['e^-4','e^-2','e^0','e^2','e^4']
pctflab=['0.89','0.67','0.44','0.22','0']
for i in range(len(range(-4,5))):
    r1=range(8,-1,-1)
    for j in range(10):
        r2=range(9,-1,-1)
        nnzlamS=benchdict[(lnlamS[i],pctfused[j])].values()[0][0]
        zlamS=benchdict2[(0,pctfused[j])].values()[0][0]
        a[r1[i],j]=nnzlamS-zlamS
fig=plt.figure()
ax=fig.add_subplot(111)
cax=ax.imshow(a,interpolation='nearest')
fig.colorbar(cax)
ax.set_xticklabels(['']+lamSlab)
ax.set_yticklabels(['']+pctflab)
show()

