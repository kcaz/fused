    for i in range(-4,8):
        a = z.benchthresh(lamP=[1], lamR=[0.5], lamS=[np.e**i], b1dim=(20,200), b2dim=(25,300), maxgroupsize=2, pct_fused=j, minfusestd=0.0, maxfusestd=0.0, xsamples1=25, xsamples2=25, noise1=0.3, noise2=0.3, p_falsep=0.0, p_falsen=0.0, sparse=0.75, it=5, threshit=10, k=7)
        benchdict[(i,j)]=a



